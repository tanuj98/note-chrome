{
"update_url": "https://clients2.google.com/service/update2/crx",

"manifest_version": 1,
    "name": "note",
    "description": "Note taking without external app ",
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "jquery.js", "foreground.js"
            ],
            "run_at": "document_end"
        }
    ],
	"permissions": [
    "storage"
  ]
}